{
    "compilerOptions": {
        "target": "es2017",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "out/js",
        "allowJs": false,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "baseUrl": "./",
        "paths": {
            "common/*": [
                "src/common/*"
            ]
        }
    }
}